{
  "name": "@teslemetry/n8n-nodes-teslemetry",
  "version": "0.1.0",
  "description": "n8n.io integration for Teslemetry",
  "main": "dist/index.cjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsdown src/index.ts --format cjs",
    "dev2": "pnpm build && mkdir -p .n8n-home/nodes && ln -sf $(pwd) .n8n-home/nodes/teslemetry && N8N_LOG_OUTPUT=console N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true N8N_USER_FOLDER=$(pwd)/.n8n-home ./node_modules/.bin/n8n start",
    "tsc": "tsc",
    "validate": "n8n-node lint",
    "dev": "n8n-node dev"
  },
  "keywords": [
    "n8n",
    "tesla",
    "teslemetry"
  ],
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Teslemetry/typescript-teslemetry"
  },
  "bugs": "https://github.com/Teslemetry/typescript-teslemetry/issues/new?template=n8n_bug.yml",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "credentials": [
      "dist/index.cjs"
    ],
    "nodes": [
      "dist/index.cjs"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.17.0",
    "@teslemetry/api": "workspace:*",
    "n8n": "^1.122.5",
    "n8n-workflow": "^1.119.1",
    "sqlite3": "^5.1.7",
    "tsdown": "^0.16.7"
  }
}
