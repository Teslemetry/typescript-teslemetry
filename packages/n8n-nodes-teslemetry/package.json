{
  "name": "@teslemetry/n8n-nodes-teslemetry",
  "version": "0.0.1",
  "description": "n8n.io integration for Teslemetry",
  "main": "dist/index.cjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsdown src/index.ts --format cjs",
    "dev": "pnpm build && mkdir -p .n8n-home/nodes && ln -sf $(pwd) .n8n-home/nodes/teslemetry && N8N_LOG_OUTPUT=console N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true N8N_USER_FOLDER=$(pwd)/.n8n-home pnpx n8n start",
    "tsc": "tsc",
    "pack": "npm pack"
  },
  "keywords": [
    "n8n",
    "tesla",
    "teslemetry"
  ],
  "author": "",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "credentials": [
      "dist/index.cjs"
    ],
    "nodes": [
      "dist/index.cjs"
    ]
  },
  "devDependencies": {
    "@teslemetry/api": "workspace:*",
    "n8n-workflow": "^1.119.1",
    "tsdown": "^0.16.7"
  }
}
